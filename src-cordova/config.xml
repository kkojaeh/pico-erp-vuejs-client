<?xml version='1.0' encoding='utf-8'?>
<widget id="pico.erp.client.vuejs" version="1.0.8" xmlns="http://www.w3.org/ns/widgets">
  <name>pico-erp</name>
  <description>pico-erp vuejs client</description>
  <author email="system@kd-ace.co.kr" href="http://acepk.co.kr/">
    에이스팩 개발자 팀
  </author>
  <content src="index.html"/>
  <plugin name="cordova-plugin-whitelist" spec="1"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <plugin name="cordova-plugin-ble-central" spec="^1.1.9"/>
  <plugin name="phonegap-plugin-media-stream" spec="^1.2.1"/>
  <engine name="android" spec="~6.4.0"/>
</widget>
