{
  "preset": "google",
  "maximumLineLength": 120,
  "disallowTrailingComma": true,
  "fileExtensions": [
    ".js"
  ],
  "excludeFiles": [
    "src/main.js",
    "src/router.js"
  ],
  "jsDoc": {
    "enforceExistence": false
  },
  "disallowSpacesInAnonymousFunctionExpression": false,
  "disallowSpacesInFunctionExpression": false
}